<html>
    <head>
        <title>MovieSearch</title>
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css">   <!-- bootstrap CDN - v4.1.3 -->
    </head>
    <body>
        <h1> 10 Movie by search: </h1>
        <a href="/">Search Again!!</a>
        
            <ol><font size="5">
                <% data["Search"].forEach(function(movie) { %>
                    <li>
                        <img height=150px src="<%= movie["Poster"]; %>">
                        <strong><%= movie["Title"]; %></strong> - 
                            <em>{<%= movie["Year"] %>}</em>
                        <br/> 
                   </li>
                    ------------------------------------------------------------------------------
                <% }); %>
             </font></ol>
        
        <a href="/">Search Again!!</a>
        
    </body>
</html>